{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev --port 9000 ",
    "cypress:run": "npx cypress run",
    "test": "start-server-and-test start http://localhost:9000 cypress:run",
    "k6": "k6.exe version",
    "k6:run": "k6.exe run",
    "k6:login": "k6.exe login cloud --config ./k6.config.json --token",
    "k6:cloud": "k6.exe cloud --config ./k6.config.json --fix",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{astro,ts}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/check": "0.7.0",
    "@astrojs/netlify": "5.2.1",
    "@astrojs/tailwind": "5.1.0",
    "astro": "4.8.7",
    "pdf-lib": "1.17.1",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  },
  "devDependencies": {
    "@types/k6": "0.51.0",
    "@typescript-eslint/parser": "8.3.0",
    "cypress": "13.10.0",
    "eslint": "9.9.1",
    "eslint-plugin-astro": "^1.2.3",
    "husky": "8.0.3",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "start-server-and-test": "2.0.3"
  }
}
